{
    "version":2,
    "builds":[
        {
            "src":"textsummarization/wsgi.py",
            "use":"@vercel/python",
            "config":{"maxLambdaSize":"15mb","runtime":"python3.12"}
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/staic_build",
            "config":{
                "distDir":"staticfile_build"
            }
        }
    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"textsummarization/wsgi.py"
        }
    ]
}